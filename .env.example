# Rename this file to .env and fill values before running

# AgileTest API
AGILETEST_AUTH_BASE_URL=https://agiletest.atlas.devsamurai.com
AGILETEST_API_BASE_URL=https://agiletest.atlas.devsamurai.com
AGILETEST_CLIENT_ID=your_client_id
AGILETEST_CLIENT_SECRET=your_client_secret
AGILETEST_PROJECT_ID=10002
AGILETEST_LIMIT=500

# Optional Jira base URL (only for links to issues)
JIRA_BASE_URL=https://your-domain.atlassian.net

# Static generation output
OUTPUT_FILE=public/data.json

PORT=3000
